<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="domain_entries">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="domain_entry" minOccurs="0" maxOccurs="1">
          <xsd:complexType>
            <xsd:simpleContent>
              <xsd:extension base="xsd:string">
                <xsd:attribute name="project" type="xsd:normalizedString" use="required"/>
                <xsd:attribute name="scope" type="xsd:normalizedString" use="required"/>
                <xsd:attribute name="domain" type="xsd:normalizedString" use="required"/>
                <xsd:attribute name="availability_zone" type="xsd:normalizedString" use="required"/>
              </xsd:extension>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
